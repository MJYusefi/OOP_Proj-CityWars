<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="310.0" prefWidth="387.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/21" fx:controller="View.EditMenu">
    <Text layoutX="20.0" layoutY="30.0" styleClass="heading-text" text="Edit Your Information">
      <font>
         <Font name="Arial Rounded MT Bold" size="19.0" />
      </font></Text>

    <Text layoutX="20.0" layoutY="77.0" text="Username:">
      <font>
         <Font name="Britannic Bold" size="14.0" />
      </font></Text>
    <TextField fx:id="usernameField" layoutX="150.0" layoutY="60.0" prefWidth="200.0" stylesheets="@../CSS/styles.css"/>

    <Text layoutX="21.0" layoutY="127.0" text="Password:">
      <font>
         <Font name="Britannic Bold" size="14.0" />
      </font></Text>
    <PasswordField fx:id="passwordField" layoutX="150.0" layoutY="110.0" prefWidth="200.0" stylesheets="@../CSS/styles.css"/>

    <Text layoutX="21.0" layoutY="177.0" text="Email:">
      <font>
         <Font name="Britannic Bold" size="14.0" />
      </font></Text>
    <TextField fx:id="emailField" layoutX="150.0" layoutY="160.0" prefWidth="200.0" stylesheets="@../CSS/styles.css"/>

    <Text layoutX="20.0" layoutY="227.0" text="Nickname:">
      <font>
         <Font name="Britannic Bold" size="14.0" />
      </font></Text>
    <TextField fx:id="nicknameField" layoutX="150.0" layoutY="210.0" prefWidth="200.0" stylesheets="@../CSS/styles.css"/>

    <Button fx:id="saveButton" layoutX="150.0" layoutY="260.0" styleClass="action-button" text="Save Changes" stylesheets="@../CSS/styles.css"/>
    <Button fx:id="cancelButton" layoutX="300.0" layoutY="260.0" text="Cancel" stylesheets="@../CSS/styles.css"/>
</AnchorPane>
